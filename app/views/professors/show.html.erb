<% @professors.each do |professor| %>
    <section id="professor-<%= professor.id %>">
        <h1>Professor: <%= professor.name %></h1>
        <% professor.student.each do |student| %>
            <section id="student-<%= student.id %>">
                <p> Students: <%= students.name %></p>
            </section>
        <% end %>
    </section>
<% end %>

